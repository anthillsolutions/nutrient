#!/usr/bin/env node
'use strict';

var app = require('../server.js');
var port = require('../utils/port.js');
var http = require('http');

var port = port.getNormalizedPort(process.env.PORT);
app.set('port', port);

var server = http.createServer(app);

server.listen(port, () => {
  console.log('Listening on port ' + port);
});
